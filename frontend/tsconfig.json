{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es6"
    ],
    "jsx": "react",
    "module": "commonjs",
    "rootDir": ".",
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true
  },
  "extends": "expo/tsconfig.base"
}
